{
  "name": "@sheenapps/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:safe": "rm -rf .next/cache && next dev -p 3000",
    "dev:webpack": "next dev --webpack -p 3000",
    "dev:clean": "rm -rf .next && next dev",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:all": "rm -rf .next node_modules package-lock.json && npm install",
    "build": "npm run generate:template-css && next build",
    "build:webpack": "npm run generate:template-css && next build --webpack",
    "generate:template-css": "node scripts/generate-template-css.js",
    "generate:i18n": "tsx scripts/generate-message-loader.ts",
    "start": "next start",
    "lint": "eslint src/",
    "lint:errors": "eslint src/ --max-warnings 0",
    "lint:critical": "eslint src/ --quiet",
    "lint:clean": "eslint src/ 2>&1 | grep -E '(Error|error|Error:|✖)' || echo 'No critical errors found'",
    "lint:fix": "eslint src/ --fix",
    "type-check": "tsc --noEmit",
    "check": "npm run lint:clean && npm run type-check && npm run validate-translations && npm run check:security && npm run build",
    "check:dev": "npm run lint:critical && npm run type-check && npm run validate-translations",
    "check:strict": "npm run lint:errors && npm run type-check && npm run validate-translations && npm run build",
    "pre-commit": "npm run lint:clean && npm run type-check && npm run validate-translations && npm run build",
    "validate-translations": "node scripts/validate-translations.js",
    "check:hardcoded-colors": "node scripts/check-hardcoded-colors.js",
    "check:security": "node scripts/check-client-bundle-security.js",
    "analyze": "ANALYZE=true npm run build:webpack",
    "analyze:workspace": "ANALYZE=true npm run build:webpack && echo 'Check .next/analyze for workspace bundle details'",
    "check-bundle-size": "node scripts/check-bundle-size.js",
    "build:with-size-check": "npm run build && npm run check-bundle-size",
    "migration:dry-run": "tsx scripts/migration-dry-run.ts",
    "migration:export": "tsx scripts/export-real-projects.ts",
    "migration:validate": "tsx scripts/validate-migrated-data.ts",
    "test:performance": "vitest run --testNamePattern=Performance",
    "size-limit": "size-limit",
    "size-check": "size-limit --why",
    "migrate-console": "node scripts/migrate-console-logs.js",
    "check:colors": "grep -rn -E \"bg-\\[#|text-\\[#|#[0-9a-fA-F]{3,8}\\b|rgb\\(|hsl\\(\" src/components || echo 'No hardcoded colors found ✅'",
    "check:rtl-phys": "grep -rn -E \"\\b(ml-|mr-|pl-|pr-|left-|right-|border-(l|r)|rounded-(l|r)|text-(left|right))\\b\" src || echo 'No physical L/R classes ✅'",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:memory": "node scripts/run-memory-tests.js",
    "test:memory:extended": "node scripts/run-memory-tests.js --extended",
    "test:memory:ci": "node scripts/run-memory-tests.js --ci",
    "db:push": "npx supabase db push",
    "db:reset": "npx supabase db reset",
    "db:types": "npx supabase gen types typescript --local > src/types/supabase.ts",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:status": "npx supabase status",
    "test:smoke": "playwright test --grep @smoke --project=chromium --timeout=90000",
    "test:smoke:ui": "playwright test --grep @smoke --ui",
    "test:smoke:debug": "playwright test --grep @smoke --debug",
    "test:smoke:headed": "playwright test --grep @smoke --headed",
    "test:smoke:baseline": "playwright test --grep @smoke && node scripts/generate-baseline.js",
    "playwright:install": "playwright install chromium --with-deps",
    "e2e:local": "./scripts/local-e2e.sh",
    "e2e:local:debug": "DEBUG_MODE=true ./scripts/local-e2e.sh",
    "e2e:local:headed": "DEBUG_MODE=true ./scripts/local-e2e.sh",
    "e2e:local:watch": "DEBUG_MODE=true npx playwright test --config=playwright.config.local.js --ui --watch",
    "e2e:test-only": "npx playwright test --config=playwright.config.local.js",
    "e2e:smoke": "GREP_FILTER='Smoke Test' ./scripts/local-e2e.sh",
    "e2e:project": "GREP_FILTER='Project Creation' ./scripts/local-e2e.sh",
    "e2e:project:debug": "DEBUG_MODE=true GREP_FILTER='Project Creation' ./scripts/local-e2e.sh",
    "dev:all": "./scripts/local-e2e.sh --skip-tests",
    "test:e2e": "playwright test --project=chromium",
    "test:e2e:p0": "playwright test --grep \"P0-\" --project=chromium",
    "test:e2e:p0:debug": "TEST_TYPE=p0 playwright test --grep \"P0-\" --project=chromium --debug",
    "test:e2e:p0:headed": "TEST_TYPE=p0 playwright test --grep \"P0-\" --project=chromium --headed",
    "db:seed:test": "tsx ./tests/seed/seed.test.ts",
    "dev:test": "NEXT_PUBLIC_SITE_URL=http://localhost:3100 TEST_E2E=1 next dev -p 3100",
    "sanity:dev": "sanity dev --port 3333",
    "sanity:build": "sanity build",
    "sanity:deploy": "sanity deploy",
    "prepare": "husky"
  },
  "size-limit": [
    {
      "name": "Homepage Bundle",
      "path": "dist/static/chunks/pages/index.js",
      "limit": "250 KB"
    },
    {
      "name": "Builder Bundle",
      "path": "dist/static/chunks/pages/workspace.js",
      "limit": "250 KB"
    }
  ],
  "dependencies": {
