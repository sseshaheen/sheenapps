{
  "name": "@sheenapps/worker",
  "version": "1.0.0",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/server.ts",
    "start": "node dist/server.js",
    "test": "jest",
    "test:contracts": "jest __tests__/contracts/",
    "audit:routes": "ts-node scripts/audit-route-coverage.ts",
    "test:migration": "jest --config jest.migration.config.js",
    "test:migration:watch": "jest --config jest.migration.config.js --watch",
    "test:migration:coverage": "jest --config jest.migration.config.js --coverage",
    "typecheck": "tsc --noEmit",
    "typecheck:strict": "tsc -p tsconfig.strict.json --noEmit",
    "typecheck:strict:count": "tsc -p tsconfig.strict.json --noEmit 2>&1 | grep -c 'error TS' || echo 0",
    "lint": "tsc --noEmit",
    "check": "pnpm run typecheck && pnpm run build && pnpm run test",
    "db:migrate:ci": "node scripts/migrate-ci.js",
    "setup": "ts-node scripts/setup.ts",
    "setup:local": "bash scripts/setup-local.sh",
    "validate:env": "node scripts/validate-env.js",
    "check:conflicts": "node scripts/check-env-conflicts.js",
    "env:reference": "bash scripts/env-reference.sh",
    "env:migrate": "node scripts/migrate-env-vars.js",
    "env:update-safe": "bash scripts/update-env-safe.sh",
    "test:worker": "ts-node scripts/testWorker.ts",
    "test:direct": "ts-node scripts/testDirect.ts",
    "test:mock": "ts-node scripts/testDirectMock.ts",
    "test:claude": "ts-node scripts/testDirectClaude.ts",
    "test:simple": "ts-node scripts/testClaudeSimple.ts",
    "test:wrangler": "ts-node scripts/test-wrangler-deploy.ts",
    "test:observability": "ts-node scripts/test-observability.ts",
    "queues": "ts-node scripts/manage-queues.ts",
    "seed:admin": "tsx scripts/seed-admin-data.ts",
    "dispatch:kv:sync": "ts-node scripts/sync-dispatch-kv.ts"
  },
