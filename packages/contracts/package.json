{
  "name": "@sheenapps/api-contracts",
  "version": "0.0.0",
  "private": true,
  "description": "Shared Zod schemas and inferred types for SheenApps API contracts",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5"
  }
}
